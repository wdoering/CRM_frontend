<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.crm334307" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>CRM</name>
  <description>
        An Ionic Framework and Cordova CRM designed in Brazil.
    </description>
  <author email="wdoering@gmail.com" href="http://stackoverflow.com/users/1069763/wdoering">
      Ionic Framework Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.1.0" />
  <allow-navigation href="*" />
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <!--<allow-navigation href="file://*/*"/>-->
  <allow-navigation href="*"/>
</widget>